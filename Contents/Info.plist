<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>com.plexapp.system.services</string>
	<key>PlexPluginClass</key>
	<string>System</string>
	<key>PlexFrameworkVersion</key>
	<string>2</string>
	<key>PlexURLServices</key>
	<dict>
		<key>4oD</key>
		<dict>
			<key>URLPattern</key>
			<string>http://www.channel4.com/programmes/.*/(4od(/?#|/player/)([0-9]{7})|episode-guide/series.*/episode.*)</string>
			<key>LinkedPlugin</key>
			<string>com.plexapp.plugins.4oD</string>
			<key>TestURLs</key>
			<array>
				<string>http://www.channel4.com/programmes/black-books/4od#3110813</string>
				<string>http://www.channel4.com/programmes/black-books/4od/player/3110813</string>
