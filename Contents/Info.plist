<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>com.plexapp.system.services</string>
	<key>PlexPluginClass</key>
	<string>System</string>
	<key>PlexFrameworkVersion</key>
	<string>2</string>
	<key>PlexURLServices</key>
	<dict>
		<key>Vimeo</key>
		<dict>
		  <key>URLPattern</key>
  		<string>http://.*vimeo\.com/.*</string>
  		<key>LinkedPlugin</key>
  		<string>com.plexapp.plugins.vimeo</string>
		</dict>
		<key>YouTube</key>
		<dict>
		  <key>URLPattern</key>
		  <string>http://.*youtube\.com/(watch)?\?v=</string>
		  <key>LinkedPlugin</key>
  	  <string>com.plexapp.plugins.youtube</string>
	  </dict>
	</dict>
	<key>PlexSearchServices</key>
	<dict>
	  <key>Vimeo</key>
	  <string>com.plexapp.search.vimeo</string>
	  <key>YouTube</key>
	  <string>com.plexapp.search.youtube</string>
  </dict>
</dict>
</plist>
