		<key>Revision3</key>
		<dict>
			<key>URLPattern</key>
			<string>http://.*revision3\.com/.*</string>
			<key>LinkedPlugin</key>
			<string>com.plexapp.plugins.revision3</string>
			<key>TestURLs</key>
			<array>
				<string>http://revision3.com/theyoungturks/trump3</string>
			</array>
