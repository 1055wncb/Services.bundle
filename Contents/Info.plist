			</array>
		</dict>
		<key>TED-Talks</key>
		<dict>
			<key>URLPattern</key>
			<string>http://.*ted.com/.*</string>
			<key>LinkedPlugin</key>
			<string>com.plexapp.plugins.ted</string>
			<key>TestURLs</key>
			<array>
				<string>http://www.ted.com/talks/arvind_gupta_turning_trash_into_toys_for_learning.html</string>
