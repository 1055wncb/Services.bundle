			<string>http://www.nfb.ca/.*</string>
			<key>LinkedPlugin</key>
			<string>com.plexapp.plugins.nfbonf</string>
			<key>TestURLs</key>
			<array>
				<string>http://www.nfb.ca/film/poets_on_film_no_3/</string>
			</array>
