<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>com.plexapp.system.services</string>
	<key>PlexPluginClass</key>
	<string>System</string>
	<key>PlexFrameworkVersion</key>
	<string>2</string>
	<key>PlexURLServices</key>
	<dict>
		<key>Adobe TV</key>
		<dict>
			<key>URLPattern</key>
			<string>http://tv.adobe.com/watch/.*</string>
			<key>LinkedPlugin</key>
			<string>com.plexapp.plugins.adobetv</string>
		</dict>
		<key>Blip.tv</key>
		<dict>
			<key>URLPattern</key>
			<string>http://.*blip\.tv/file/.*</string>
			<key>LinkedPlugin</key>
			<string>com.plexapp.plugins.blip</string>
		</dict>
		<key>Vimeo</key>
		<dict>
			<key>URLPattern</key>
			<string>http://.*vimeo\.com/.*</string>
			<key>LinkedPlugin</key>
			<string>com.plexapp.plugins.vimeo</string>
		</dict>
		<key>YouTube</key>
		<dict>
			<key>URLPattern</key>
			<string>http://.*youtu(be\.com|\.be)/.*</string>
			<key>LinkedPlugin</key>
			<string>com.plexapp.plugins.youtube</string>
			<key>TestURLs</key>
			<array>
				<string>http://youtu.be/vXhmyyXFd5I</string>
				<string>http://youtube.com/watch?v=vXhmyyXFd5I</string>
				<string>http://www.youtube.com/user/andyverg#p/a/u/0/HTGOoQNYGL4</string>
			</array>
		</dict>
	</dict>
	<key>PlexSearchServices</key>
	<dict>
		<key>VideoSurf</key>
		<string>com.plexapp.search.videosurf</string>
	</dict>
</dict>
</plist>
